    <select id="findPage" resultType="com.wemirr.platform.authority.domain.vo.UserResp">
        SELECT
        *
        FROM t_user
    </select>
    <select id="list" resultType="com.github.jgzl.infra.upms.domain.entity.baseinfo.User">
